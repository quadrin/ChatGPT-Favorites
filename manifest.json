{
  "name": "ChatGPT-Favorites",
  "version": "1.0",
  "description": "Adds ability to favorite chat logs and messages to ChatGPT",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["contentScript.js", "messageFavoriting.js"]
    }
  ]
}
